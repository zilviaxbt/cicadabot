{
  "name": "cicada-bot",
  "version": "1.0.0",
  "description": "Cicada Bot - Advanced trading bot for GalaSwap DEX with prime interval trading strategy inspired by periodical cicadas",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "trading": "ts-node examples/trading-script.ts",
    "arbitrage": "ts-node examples/arbitrage-bot.ts",
    "auto-arbitrage": "ts-node examples/auto-arbitrage.ts",
    "auto-arbitrage:aggressive": "ts-node examples/auto-arbitrage.ts aggressive",
    "auto-arbitrage:conservative": "ts-node examples/auto-arbitrage.ts conservative",
    "prime": "ts-node examples/prime-interval-strategy.ts",
    "lunar": "ts-node examples/lunar-trading-bot.ts",
    "test-lunar": "ts-node examples/test-lunar-strategy.ts",
    "web": "ts-node web/server.js",
    "web:dev": "nodemon --exec ts-node web/server.js",
    "web:build": "tsc && node dist/web/server.js"
  },
  "keywords": [
    "cicada-bot",
    "galaswap",
    "defi",
    "trading",
    "bot",
    "typescript",
    "gala-chain",
    "prime-interval",
    "periodical-cicadas",
    "arbitrage",
    "lunar-trading"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@gala-chain/gswap-sdk": "^0.0.7",
    "@types/node-fetch": "^2.6.13",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
