# GalaSwap Bot Configuration
# Copy this file to .env and fill in your values

# Your private key (hex format, without 0x prefix)
PRIVATE_KEY=(allyourbase) 

# Your wallet address (eth| format)
WALLET_ADDRESS=(arebelongtous)

# Optional: Custom gateway URL (defaults to mainnet)
# GATEWAY_BASE_URL=https://gateway-mainnet.galachain.com

# Optional: Transaction timeout in milliseconds (default: 300000 = 5 minutes)
# TRANSACTION_WAIT_TIMEOUT_MS=300000

# Optional: Log level (error, warn, info, debug)
LOG_LEVEL=info
